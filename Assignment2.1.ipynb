    "def myfil (fnc,seq) :\n",
    "    l= []\n",
    "    for i in seq:\n",
    "        if fnc(i) and i>0:\n",
    "            l.append(i)\n",
    "    return l\n",
    "\n",
    "print (\"The even no:s are\")\n",
    "print (myfil((lambda x:x%2==0),[1,2,3,4,5,6,0,-2]))"
   ]
